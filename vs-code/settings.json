{
    "breadcrumbs.enabled": false,
    "scss.lint.unknownAtRules": "ignore",
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.hideCursorInOverviewRuler": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 1.3,
    "editor.lightbulb.enabled": false,
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.tabSize": 2,
    "emmet.triggerExpansionOnTab": true,
    "explorer.decorations.badges": false,
    "workbench.activityBar.visible": false,
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
    "scm.diffDecorationsGutterWidth": 1,
    "scm.diffDecorations": "gutter",
    "security.workspace.trust.untrustedFiles": "open",
    "search.exclude": {
        "**/vendor/{[^l],?[^ai]}*": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/package-lock.json": true,
        "**/tmp": true,
        "*.DS_Store": true,
        "app/assets/builds": true,
        "public/assets": true,
        "public/assets/packs": true,
        "public/packs": true,
        "public/packs-test": true,
        "storage": true,
        ".byebug_history": true,
        "development.log": true,
        "test.log": true,
        "yarn.lock": true,
        "yarn-error.log": true,
    },
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 25,
    "tailwindCSS.lint.cssConflict": "ignore",
    "files.associations": {
        "*.css": "scss",
        // "*.html.erb": "html"
    },
    "editor.occurrencesHighlight": false,
    "editor.renderLineHighlight": "none",
    "standard.run": "onSave",
    "svg.preview.mode": "svg",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "headwind.classRegex": {
        "html": "\\bclass\\s*[\\=\\:]\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\']",
        "css": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
        "scss": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
        "javascript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
        "javascriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
        "typescript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
        "typescriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)"
    },
    "headwind.prependCustomClasses": true,
    "headwind.removeDuplicates": true,
    "headwind.runOnSave": true
}